    <style name="BotaoMapa">
        <item name="android:layout_marginRight">@dimen/margin_ml</item>
        <item name="android:layout_width">@dimen/size_l</item>
        <item name="android:layout_height">@dimen/size_l</item>
    </style>

    <!-- Lista -->

    <style name="ListView" parent="MatchMatch"></style>

    <style name="ListViewMatchWrapGM" parent="MatchWrap">
        <item name="android:background">@color/cor_overlay_escuro</item>
    </style>

    <style name="ListViewSemEfeitos" parent="ListView">
        <item name="android:fadeScrollbars">false</item>
        <item name="android:scrollbarAlwaysDrawVerticalTrack">true</item>
        <item name="android:scrollbars">vertical</item>
